{
  "name": "dodo-style",
  "author": "Erdoğan Yeşil",
  "version": "0.1.4",
  "homepage": "https://github.com/erdodo/dodo-style",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "npm:build": "rm -rf dist && NODE_ENV=production babel src --out-dir dist --copy-files && npm run tailwind:build",
    "npm:build:windows": "del dist && NODE_ENV=production babel src --out-dir dist --copy-files && npm run tailwind:build",
    "npm:deploy": "npm run npm:build && npm publish --access public",
    "npm:deploy:windows": "npm run npm:build:windows && npm publish --access public",
    "npm:publish": "npm publish --access public",
    "tailwind:watch": "npx tailwindcss -i ./src/index.css -o ./assets/output.css --watch",
    "tailwind:build": "npx tailwindcss -i ./src/index.css -o ./assets/output.css && npx tailwindcss -i ./src/index.css -o ./dist/output.css",
    "storybook:serve": "storybook dev -p 6006",
    "storybook:build": " storybook build",
    "storybook:deploy": "npm run tailwind:build && npm run storybook:build && vercel --prod",
    "update": "npm run tailwind:build && npm run storybook:deploy && npm run  npm:deploy"
  },
  "dependencies": {
    "dodo-icons": "^0.1.0",
    "moment": "^2.29.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@storybook/addon-essentials": "^7.0.7",
    "@storybook/addon-interactions": "^7.0.7",
    "@storybook/addon-links": "^7.0.7",
    "@storybook/blocks": "^7.0.7",
    "@storybook/preset-create-react-app": "^7.0.7",
    "@storybook/react": "^7.0.7",
    "@storybook/react-webpack5": "^7.0.7",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-scripts": "5.0.1",
    "react-styleguidist": "^13.1.1",
    "storybook": "^7.0.7",
    "storybook-dark-mode": "^3.0.0",
    "storybook-tailwind-dark-mode": "^1.0.22",
    "tailwindcss": "^3.3.2",
    "web-vitals": "^2.1.4",
    "webpack": "^5.80.0"
  }
}
