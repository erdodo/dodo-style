{
  "name": "@erdodocomp/dodo-style",
  "version": "0.1.29",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "styleguide": "npx styleguidist server",
    "styleguide:build": "npx styleguidist build && npx tailwindcss -i ./src/index.css -o ./styleguide/output.css",
    "npm:deploy": " npm publish --access public",
    "npm:build": "rm -rf dist && NODE_ENV=production babel src/components --out-dir dist --copy-files && npx tailwindcss -i ./src/index.css -o ./dist/output.css --watch"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "react-styleguidist": "^13.1.1",
    "tailwindcss": "^3.3.2",
    "webpack": "^5.80.0"
  }
}
